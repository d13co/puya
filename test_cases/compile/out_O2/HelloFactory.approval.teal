#pragma version 10

test_cases.compile.factory.HelloFactory.approval_program:
    txn NumAppArgs
    bz main_bare_routing@5
    method "do_some_stuff()void"
    txna ApplicationArgs 0
    match main_do_some_stuff_route@2
    err // reject transaction

main_do_some_stuff_route@2:
    txn OnCompletion
    !
    assert // OnCompletion is NoOp
    txn ApplicationID
    assert // is not creating
    callsub do_some_stuff
    int 1
    return

main_bare_routing@5:
    txn OnCompletion
    !
    assert // reject transaction
    txn ApplicationID
    !
    assert // is creating
    int 1
    return


